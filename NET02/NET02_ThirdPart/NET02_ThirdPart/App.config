<?xml version="1.0" encoding="utf-8" ?>
<configuration>

	<configSections>
		<section name="CustomTargets" type="NET02_ThirdPart.CustomConfig.StartupTargetsConfigSection, NET02_ThirdPart"/>
		<section name ="CustomLogger" type="NET02_ThirdPart.CustomConfig.StartupLoggerConfigSection, NET02_ThirdPart"/>
	</configSections>


	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>


	<CustomTargets>
		<Targets>
			<add type =" TextListener.TextListener,TextListener">
				<add name ="Name" value="Ivan"/>
				<add name ="Age" value="12" />

			</add>
			<add type =" WordListener.WordListener,WordListener"/>
			<add type =" EventLogListener.EventLogListener,EventLogListener"/>
		</Targets>
	</CustomTargets>


	<CustomLogger>
		<loggers>
			<add minlevel="Trace"/>
		</loggers>
	</CustomLogger>


</configuration>
